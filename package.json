    "dataurl": "^0.1.0",
    "esbuild": "^0.14.28",
    "id3js": "^2.1.1",
    "mime-types": "^2.1.35",
    "music-metadata": "^7.12.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.2",
    "react-toastify": "^8.2.0",
    "sync": "^0.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "electron": "^17.2.0",
    "electron-packager": "^15.4.0",
    "vite": "^2.8.0"
